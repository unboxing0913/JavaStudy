package part01.sec01.exam01;

public class Ex07_05 {

	public static void main(String[] args) {
		int a=100;
		
		while(a == 200) { //(먼저실행) 참일경우반복
			System.out.println("while 문 내부에 들어 왔습니다.");
		}
		
		do { // 먼저실행후 while은 나중실행 (실행하고 비교) 선 처리 후 비교
			System.out.println("do~while 문 내부에 들어 왔습니다.");
		}while(a == 200);
			
			
		
	
		
	}

}
